{
  "name": "wordle-friends-backend",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run pack:postconfirmation && npm run pack:api",
    "format": "prettier --write .",
    "lint": "eslint --ext ts lambdas",
    "lint:fix": "npm run lint -- --fix",
    "pack:api": "npm --prefix ./lambdas/api install && rm -f ./lambdas/api.zip && cd ./lambdas/api && zip -r -X ../api.zip .",
    "pack:postconfirmation": "npm --prefix ./lambdas/postConfirmation install && rm -f ./lambdas/postConfirmation.zip && cd ./lambdas/postConfirmation && zip -r -X ../postConfirmation.zip .",
    "watch": "tsc -w"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "aws-lambda": "^1.0.7",
    "eslint": "^8.9.0",
    "eslint-config-wordle-friends": "^0.0.1",
    "eslint-plugin-import": "^2.25.4",
    "prettier": "^2.5.1"
  }
}
